    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
    },
    "editor.formatOnSave": true,
    "eslint.format.enable": true,
    "typescript.format.enable": false,
    "javascript.format.enable": false,
}